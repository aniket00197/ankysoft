<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<HBox xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
	<children>
		<AnchorPane prefHeight="300.0" prefWidth="462.0">
			<children>
				<TextField fx:id="username" layoutX="176.0" layoutY="83.0" prefHeight="25.0" prefWidth="182.0" promptText="Enter your user name" />
				<PasswordField fx:id="password" layoutX="176.0" layoutY="138.0" prefHeight="25.0" prefWidth="182.0" promptText="Enter your password" />
				<Button layoutX="49.0" layoutY="210.0" mnemonicParsing="false" onAction="#login" prefHeight="25.0" prefWidth="75.0" text="Login" />
				<Button layoutX="180.0" layoutY="210.0" mnemonicParsing="false" onAction="#signUp" prefHeight="25.0" prefWidth="75.0" text="Sign Up" />
				<Button layoutX="283.0" layoutY="210.0" mnemonicParsing="false" onAction="#close" prefHeight="25.0" prefWidth="75.0" text="Cancel" />
				<Label layoutX="49.0" layoutY="87.0" prefHeight="17.0" prefWidth="75.0" text="UserName" />
				<Label layoutX="49.0" layoutY="142.0" prefHeight="17.0" prefWidth="75.0" text="Password" />
				<Label fx:id="loginMessage" layoutX="49.0" layoutY="261.0" prefHeight="17.0" prefWidth="337.0" text="Label" />
			</children>
		</AnchorPane>
	</children>
</HBox>
