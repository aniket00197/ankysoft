					<padding>
						<Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
					</padding>
				</TextField>
				<TextField fx:id="lastname" layoutX="186.0"
					layoutY="220.0" prefHeight="26.0" prefWidth="216.0">
					<padding>
						<Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
					</padding>
				</TextField>
				<TextField fx:id="emailid" layoutX="186.0" layoutY="279.0"
