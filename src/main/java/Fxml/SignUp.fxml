				<Label layoutX="39.0" layoutY="283.0" prefHeight="17.0"
					prefWidth="87.0" text="Email ID" />
				<Label layoutX="39.0" layoutY="332.0" prefHeight="17.0"
					prefWidth="87.0" text="Phone Number" />
				<TextField fx:id="username" layoutX="186.0" layoutY="70.0"
					prefHeight="26.0" prefWidth="216.0">
					<padding>
						<Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
					</padding>
				</TextField>
				<PasswordField fx:id="password" layoutX="186.0"
					layoutY="120.0" prefHeight="26.0" prefWidth="216.0">
					<padding>
						<Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
					</padding>
				</PasswordField>
				<TextField fx:id="firstname" layoutX="186.0"
