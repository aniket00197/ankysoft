						<Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
					</padding>
				</TextField>
				<RadioButton layoutX="186.0" layoutY="398.0"
					mnemonicParsing="false" prefHeight="17.0" prefWidth="87.0"
					text="Male" />
				<RadioButton layoutX="319.0" layoutY="398.0"
					mnemonicParsing="false" prefHeight="17.0" prefWidth="87.0"
					text="Female" />
				<Button layoutX="39.0" layoutY="460.0"
					mnemonicParsing="false" onAction="#signUp" prefHeight="25.0"
					prefWidth="87.0" text="Submit" />
				<Button layoutX="186.0" layoutY="460.0"
					mnemonicParsing="false" prefHeight="25.0" prefWidth="87.0"
